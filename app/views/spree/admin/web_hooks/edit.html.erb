<% content_for :page_title do %>
    <%= Spree.t(:edit_web_hook) %>
<% end %>

<% content_for :page_actions do %>
    <%= button_link_to Spree.t(:new_web_hook), new_admin_web_hook_url, {:class => "btn-success", :icon => 'add'} %>
<% end %>

<%= render :partial => 'spree/admin/shared/error_messages', :locals => {:target => @web_hook} %>

<%= form_for [:admin, @web_hook],url: admin_web_hook_url, :method => :put, :html => {:multipart => true} do |f| %>
    <fieldset>
      <%= render :partial => 'form', :locals => {:f => f} %>
      <%= render :partial => 'spree/admin/shared/edit_resource_links', locals: {collection_url: admin_web_hooks_url} %>
    </fieldset>
<% end %>
